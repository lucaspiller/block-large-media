{
  "manifest_version": 2,
  "name": "Block Large Media",
  "description": "",
  "version": "0.1",
  "background": {
    "scripts": ["js/blocker.js"],
    "persistent": true
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "background",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
